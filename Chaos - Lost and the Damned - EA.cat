    <rule id="9b7f-a16c-efaa-c053" name="Daemonic Pact" hidden="false">
      <modifiers>
        <modifier type="set" field="description" value="Pacte Démoniaque: Il permet la formation de convoquer des démons.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="798a-d9b8-47b6-c74c" type="equalTo"/>
