    <profile id="2633-feb5-3128-e57c" profileTypeId="f68a073b-c562-ca93-961e-5cf373ae14cb" name="Drone Sentry Turret P" hidden="false">
      <characteristics>
        <characteristic characteristicId="be964774-1659-c1e9-573f-be7c258d4b74" name="Type" value="Light Vehicle"/>
