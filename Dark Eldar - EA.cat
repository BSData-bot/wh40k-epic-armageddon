          </conditions>
          <conditionGroups/>
        </modifier>
      </modifiers>
    </rule>
    <rule id="ef57-e9ae-e813-7abe" name="&lt;I&gt;F-ERC&lt;/I&gt;" hidden="false">
      <description>&lt;I&gt;eldars noirs codex #FERC BETA-TEST 10/09/13&lt;/I&gt;


&lt;I&gt;&lt;B&gt;:&lt;/B&gt; &lt;/I&gt;</description>
      <modifiers/>
