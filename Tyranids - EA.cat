    <rule id="80c7-09aa-974e-9386" name="&lt;I&gt;Déploiement&lt;/I&gt;" hidden="true">
      <description>&lt;I&gt;Seuls les essaims indépendants peuvent se déployer en garnison, s’ils respectent la règle le permettant.&lt;/I&gt;</description>
      <modifiers>
        <modifier type="show" field="name" value="0.0" repeat="false" numRepeats="1" incrementParentId="roster" incrementChildId="no child" incrementField="selections" incrementValue="1.0">
